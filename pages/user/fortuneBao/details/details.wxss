/* pages/wo/caifubao/mingxi/mingxi.wxss */
page{
  background-color:#f7f7f7;
}
.top{
  width:100%;
  height:1px;
  background-color:#e0e0e0;
}
.line{
  width:100%;
  height:50px;
  line-height: 50px;
  background-color:white;
  border-bottom:1px solid #e0e0e0;
}
.left{
  float:left;
  min-width:120px;
  height:100%;
  margin-left:30rpx;
}
.title{
  float:left;
  width:100%;
  height:30px;
  line-height:30px;
  color:#353535;
  font-size:11pt;
}
.time{
  float:left;
  width:100%;
  height:20px;
  line-height:20px;
  margin-top:-5px;
  color:#888;
  font-size:9pt;
}
.right{
  float:right;
  width:auto;
  margin-right:20rpx;
}
.right-balck{
  color:#353535;
  font-size:11pt;
}
.right-green{
  color:green;
  font-size:11pt;
}