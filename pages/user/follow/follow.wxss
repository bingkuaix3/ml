/* follow.wxss */
page {
  background-color: #f7f7f7;
  height: 100%;
}

.line {
  width: 100%;
  height:90rpx;
  border-bottom: 1px solid #f0f0f0;
  display: flex;
  background-color:white;
}

.head {
  width: 80rpx;
  height:80rpx;
  border-radius: 40rpx;
  margin: 5rpx 20rpx 5rpx 30rpx;
}

.name {
  width: auto;
  flex: 1;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 10pt;
  color: #353535;
}

.time {
  width: auto;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 10pt;
  color: #666;
  margin-right:30rpx;
}
.cancle{
  height:48rpx;
  margin-top:20rpx;
  margin-left:30rpx;
  font-size:9pt;
  color:white;
  text-align:center;
  line-height:48rpx;
}