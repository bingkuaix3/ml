page {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
  margin: 0;
  padding: 0;
  font-size: 13pt;
  color: #09bb07;
  font-weight: 700;
}

.card1 {
  position: relative;
  padding-top: 60rpx;
}

.card {
  position: relative;
  margin: 40rpx 0;
}

.img {
  display: block;
  width: 100%;
  height: 300rpx;
  margin: 0;
  padding: 0;
}

text {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  line-height: 1.4;
  letter-spacing: 2rpx;
}

.text1 {
  position: absolute;
  top: 80rpx;
  right: 20rpx;
  line-height: 1.4;
  letter-spacing: 2rpx;
}
