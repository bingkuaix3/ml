/* applyFor.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
  color: #888;
  margin: 0;
  padding: 0;
}

.originator {
  background-color: rgba(0, 0, 0, 0.6);
  padding: 30rpx 30rpx;
  color: #fff;
  font-size: 13pt;
}
.overFlowText {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:3;
  overflow: hidden;
}
.describe {
  line-height: 1.4;
  letter-spacing: 1.2rpx;
  margin-top: 30rpx;
  font-size: 11pt;
}
.labelOne{
  margin: 30rpx 0 16rpx 30rpx;
  font-size: 13pt;
  color: #353535;
}
.label{
 padding-bottom: 16rpx ;
  font-size: 11pt;
  color: #353535;
  border-bottom: 1rpx solid #f7f7f7;
}
.card {
  background-color: #fff;
  margin-bottom: 40rpx;
  padding: 30rpx 30rpx;
}
.text{
  line-height: 1.4;
  letter-spacing: 1.2rpx;
  font-size: 9pt;
  margin-top: 30rpx;
}
.flex{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 11pt;
  border-bottom: 1rpx solid #f7f7f7;
  padding: 30rpx 30rpx;
}
.textRight{
  width: 180rpx;
  text-align: right;
}
.labelDown{
  font-size: 11pt;
  color: #353535;
  flex:1;
}
.cardDown {
  background-color: #fff;
  margin-bottom: 40rpx;
}
.switch{
 transform:scale(0.8)
}
.btn-shenqing{
  height:94rpx;
  margin:30rpx 60rpx 30rpx 60rpx;
}
.btn-shenqin button{
  width:100%;
  height:94rpx;
  border-radius:10rpx;
  color:white;
  font-size:13pt;
}