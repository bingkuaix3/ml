<!--privatechatList.wxml-->
<block wx:for="{{list}}">
<view bindtap="goToPrivatechat" data-id="{{item.serialnumber}}" data-user="{{item.userid}}" class="line">
  <image class="head" src="{{item.headpic}}"></image>
  <view class="name">{{item.xingming}}</view>
  <view class="time">{{item.counts}}</view>
</view>
</block>
