page {
  height: 100%;
  background-color: #f7f7f7;
}

.top {
  width: 100%;
  height: 1px;
  background-color: f0f0f0;
}

.box {
  width: 100%;
  height: auto;
  background-color: white;
}

.line {
  width: auto;
  height: 90rpx;
  display: flex;
  margin: 0 30rpx;
}
.temp{
  border-bottom:1px solid #f0f0f0;
}
.time {
  width: auto;
  font-size: 11pt;
  line-height:90rpx;
  color: #353535;
  flex:1;
  letter-spacing: 1.2rpx;
}

.status {
  width: 20rpx;
  height: 20rpx;
  border-radius: 20rpx;
  background-color: #09bb07;
  margin: 35rpx 0rpx 35rpx 30rpx;
}

.content {
  min-height: 90rpx;
  margin: 0rpx 30rpx 0rpx 30rpx;
  font-size: 10pt;
  color: #888;
}

.content-title2 {
 font-size:9pt;
  margin-top:5rpx;
  min-height:40rpx;
}

.content-content {
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  width: 100%;
  line-height: 1.4;
  letter-spacing: 1.2rpx;
}
.cc-box{
  width:100%;
  display: flex;
  justify-content: center;
  padding-top:20rpx;
  padding-bottom:20rpx;
}
.cancle{
  width:130rpx;
  height:60rpx;
  font-size:10pt;
  color:#888;
  text-align:center;
  line-height:60rpx;
  margin-right:50rpx;
  background-color: #fff;
}
.accept{
  width:130rpx;
  height:60rpx;
  text-align:center;
  line-height:60rpx;
  font-size:9pt;
  color:white;
  margin-left:50rpx;
  letter-spacing: 1.2rpx;
  
}

