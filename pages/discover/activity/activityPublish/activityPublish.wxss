
page {
  width: 100%;
  height: 100%;
  font-size: 11pt;
  margin: 0;
  padding: 0;
  background-color: #efeff4;
}

.card {
  background-color: #fff;
}

.name {
  font-size: 11pt;
  color: #888;
  margin: 0 30rpx;
  padding: 20rpx 0;
  background-color: #fff;
  border-bottom: 1rpx solid #f7f7f7;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 26rpx;
}

.icon {
  display: block;
  width: 40rpx;
  height: 35rpx;
  margin-top: 5px;
}

.Icon {
  display: block;
  width: 50rpx;
  height: 50rpx;
}

.text {
  margin: 0 30rpx;
  padding-top: 30rpx;
  padding-bottom: 16rpx;
  color: #888;
  font-size:11pt;
  text-align: left;
}

.middle {
  text-align: left;
  margin: 0;
  margin-top: 60rpx;
  margin-bottom: 40rpx;
  flex:1;
}

.body-view {
  background-color: #fff;
  line-height: 90rpx;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-content: center;
  padding: 0 30rpx;
  color: #353535;
  font-size: 13pt;
}

.big {
  transform: scale(0.8);
}

.block {
  display: block;
}

.none {
  display: none;
}

.btn {
  position: fixed;
  width: 100%;
  bottom: 200rpx;
  font-size: 11pt;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.btnRight {
  height: 70rpx;
  width: 240rpx;
  background-color: #09bb07;
  border: 1rpx solid #f7f7f7;
  border-radius: 8rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
}

.btnLeft {
  height: 70rpx;
  width: 240rpx;
  background-color: #fff;
  border: 1rpx solid #09bb07;
  border-radius: 8rpx;
  line-height: 70rpx;
  text-align: center;
  color: #09bb07;
}

.list {
  display: flex;
  flex-direction: row;
  font-size: 11pt;
  color: #888;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #f7f7f7;
  background-color: #fff;
}

.inputs {
  height:26px;
  line-height: 26px;
  font-size: 13pt;
  margin-top: 0px;
}

.names {
  width: 105px;
  height: 26px;
  line-height: 26px;
  font-size: 13pt;
  color: #353535;
}

.shanchu {
  height: 400rpx;
  margin: 60rpx 30rpx;
  position: relative;
}

.img {
  width: 100%;
  height: 400rpx;
}

.shanchuIcon {
  display: block;
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: -20rpx;
  right: -20rpx;
}

.btnUp {
  height: 60rpx;
  line-height: 60rpx;
  width: 120rpx;
  font-size: 9pt;
  margin-top: 80rpx;
}


.flex {
  margin: 0 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
